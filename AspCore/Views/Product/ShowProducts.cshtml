@model IEnumerable<PresentationLayer.Models.View.ProductViewModel>

<ul>
    @if (Model.Count() != 0)
    {
        @foreach (var product in Model)
        {
            <li>
                @product.CategoryType
                @product.Price
                @product.Description
            </li>
        }
    }
    else
    {
        <label> Not found products in the database. Change your search criteria</label>
    }
</ul>