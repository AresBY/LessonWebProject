@model IEnumerable<AspCore.Models.ProductModel>

<ul>
    @if (Model.Count() != 0)
    {
        @foreach (var product in Model)
        {
            <li>
                @product.Type
                @product.Name
                @product.Price
            </li>
        }
    }
    else
    {
        <label> Not found products in the database. Change your search criteria</label>
    }
</ul>